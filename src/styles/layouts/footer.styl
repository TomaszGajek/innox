footer
  margin-top 50px
  background #97735e
  display flex
  flex-direction column

  h4
    text-transform uppercase
    color white
    letter-spacing: 1.2px
    text-align: center
    padding-bottom 100px

.footer__logo
  margin 0 auto
  padding 70px 0 30px

.footer__icons
  margin 0 auto
  display flex
  flex-direction row
  div
    height 25px
    width  25px
    border-radius 50%
    display flex
    transition all 0.5s
    margin-right 10px

    &:hover
      background #a88a78
    img
      margin auto
      z-index 2
